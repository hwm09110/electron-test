{
  "name": "electron-demo",
  "version": "0.0.15",
  "description": "electron-test",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./build/dev-runner.js",
    "dev:web": "cross-env NODE_ENV=development BUILD_TARGET=web node ./build/dev-runner.js",
    "build": "cross-env NODE_ENV=production node ./build/index.js && electron-builder",
    "build:web": "cross-env NODE_ENV=production BUILD_TARGET=web node ./build/index.js",
    "clean": "cross-env BUILD_TARGET=clean node ./build/index.js"
  },
  "keywords": [
    "electron"
  ],
  "author": "weimingHuang",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.23.2",
    "babel-loader": "^8.2.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^3.2.0",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "electron": "^18.2.0",
    "electron-builder": "^20.19.2",
    "electron-reloader": "^1.2.3",
    "electron-updater": "^6.1.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^4.5.2",
    "less-loader": "^11.1.3",
    "listr": "^0.14.3",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "style-loader": "2.0.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.11.1",
    "vue-style-loader": "^4.1.3",
    "webpack": "^4.47.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "@electron/remote": "^2.1.0",
    "core-js": "^3.33.1",
    "electron-log": "^5.0.0",
    "view-design": "^4.7.0",
    "vue": "2.7.14",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "build": {
    "productName": "electron-demo",
    "appId": "456789",
    "directories": {
      "output": "pack"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraResources": [
      {
        "from": "./public",
        "to": "./public"
      }
    ],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "public/icons/icon.ico",
      "uninstallerIcon": "public/icons/icon.ico",
      "installerHeaderIcon": "public/icons/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "electron-demo",
      "artifactName": "${productName}-setup-v${version}.${ext}",
      "uninstallDisplayName": "${productName}"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "public/icons/icon.icns"
    },
    "win": {
      "icon": "public/icons/icon.ico",
      "artifactName": "${productName}_Setup_${version}.${ext}",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "icon": "public/icons"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "hwm09110",
        "repo": "electron-test"
      }
    ]
  }
}